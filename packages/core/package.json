{
  "name": "@cryptoandcoffee/akash-jsdk-core",
  "version": "3.4.0",
  "description": "Core SDK for Akash Network",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@cosmjs/amino": "^0.37.0",
    "@cosmjs/encoding": "^0.37.0",
    "@cosmjs/proto-signing": "^0.37.0",
    "@cosmjs/stargate": "^0.37.0",
    "@cryptoandcoffee/akash-jsdk-protobuf": "workspace:*",
    "elliptic": "^6.5.4",
    "isomorphic-ws": "^5.0.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@vitest/coverage-v8": "^4.0.9",
    "js-yaml": "^4.1.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.9"
  }
}
