{
  "name": "@cryptoandcoffee/akash-jsdk-core",
  "version": "1.0.1",
  "description": "Core SDK for Akash Network",
  "type": "module",
  "keywords": [
    "akash",
    "network",
    "blockchain",
    "cosmos",
    "sdk",
    "typescript"
  ],
  "homepage": "https://github.com/cryptoandcoffee/akash-jsdk#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/cryptoandcoffee/akash-jsdk.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/cryptoandcoffee/akash-jsdk/issues"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "CryptoAndCoffee",
    "url": "https://github.com/cryptoandcoffee"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "format:check": "prettier --check src/**/*.{ts,tsx}",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@cosmjs/encoding": "^0.32.2",
    "@cosmjs/proto-signing": "^0.32.2",
    "@cosmjs/stargate": "^0.32.2",
    "@cryptoandcoffee/akash-jsdk-protobuf": "workspace:*"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@vitest/coverage-v8": "^1.6.1",
    "js-yaml": "^4.1.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.2.0"
  }
}
