{
  "name": "@cryptoandcoffee/akash-jsdk-react",
  "version": "3.6.2",
  "description": "React hooks for Akash Network SDK",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@cryptoandcoffee/akash-jsdk-core": "workspace:*"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.9",
    "jsdom": "^27.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.9"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "keywords": [
    "akash",
    "akash-network",
    "react",
    "react-hooks",
    "hooks",
    "blockchain",
    "cosmos",
    "web3",
    "decentralized-cloud",
    "deployment",
    "provider",
    "typescript",
    "frontend",
    "ui"
  ],
  "homepage": "https://github.com/cryptoandcoffee/akash-jsdk/tree/main/packages/react#readme",
  "bugs": {
    "url": "https://github.com/cryptoandcoffee/akash-jsdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cryptoandcoffee/akash-jsdk.git",
    "directory": "packages/react"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "CryptoAndCoffee",
    "url": "https://github.com/cryptoandcoffee"
  },
  "publishConfig": {
    "access": "public"
  }
}
