{
  "name": "@cryptoandcoffee/akash-jsdk-protobuf",
  "version": "3.6.2",
  "description": "Protobuf definitions for Akash Network",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/src/index.d.ts"
    }
  },
  "files": [
    "dist",
    "proto"
  ],
  "scripts": {
    "build": "npm run proto:generate && vite build",
    "proto:download": "node scripts/download-protos.js",
    "proto:generate": "node scripts/generate-proto.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "npm run build -- --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist generated proto-downloaded"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.10.1",
    "@bufbuild/protoc-gen-es": "^2.10.1"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.28.1",
    "@vitest/coverage-v8": "^4.0.9",
    "c8": "^10.1.3",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.9"
  },
  "keywords": [
    "akash",
    "akash-network",
    "protobuf",
    "protocol-buffers",
    "grpc",
    "blockchain",
    "cosmos",
    "cosmos-sdk",
    "types",
    "typescript",
    "bufbuild",
    "ibc",
    "deployment",
    "staking"
  ],
  "homepage": "https://github.com/cryptoandcoffee/akash-jsdk/tree/main/packages/protobuf#readme",
  "bugs": {
    "url": "https://github.com/cryptoandcoffee/akash-jsdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cryptoandcoffee/akash-jsdk.git",
    "directory": "packages/protobuf"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "CryptoAndCoffee",
    "url": "https://github.com/cryptoandcoffee"
  },
  "publishConfig": {
    "access": "public"
  }
}
