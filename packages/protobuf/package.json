{
  "name": "@cryptoandcoffee/akash-jsdk-protobuf",
  "version": "1.0.0",
  "description": "Protobuf definitions for Akash Network",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/src/index.d.ts"
    }
  },
  "files": [
    "dist",
    "proto"
  ],
  "scripts": {
    "build": "npm run proto:generate && vite build",
    "proto:download": "node scripts/download-protos.js",
    "proto:generate": "node scripts/generate-proto.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "npm run build -- --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist generated proto-downloaded"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.7.2",
    "@bufbuild/protoc-gen-es": "^1.7.2"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.28.1",
    "@vitest/coverage-v8": "^1.6.1",
    "c8": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.2.0"
  }
}
