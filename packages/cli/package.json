{
  "name": "@cryptoandcoffee/akash-jsdk-cli",
  "version": "1.0.0",
  "description": "Command-line tools for Akash Network SDK",
  "type": "module",
  "keywords": ["akash", "network", "cli", "blockchain", "cosmos", "deployment"],
  "homepage": "https://github.com/cryptoandcoffee/akash-jsdk#readme",
  "repository": {
    "type": "git", 
    "url": "https://github.com/cryptoandcoffee/akash-jsdk.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/cryptoandcoffee/akash-jsdk/issues"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "CryptoAndCoffee",
    "url": "https://github.com/cryptoandcoffee"
  },
  "bin": {
    "akash-cli": "./dist/cli.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "format:check": "prettier --check src/**/*.{ts,tsx}",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@cryptoandcoffee/akash-jsdk-core": "file:../core",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@vitest/coverage-v8": "^1.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.2.0"
  }
}
