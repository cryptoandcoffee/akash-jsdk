{
  "name": "@cryptoandcoffee/akash-jsdk-cli",
  "version": "3.6.2",
  "description": "Command-line tools for Akash Network SDK",
  "type": "module",
  "bin": {
    "akash-cli": "./dist/cli.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@cryptoandcoffee/akash-jsdk-core": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^14.0.2",
    "inquirer": "^12.11.1",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.9",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.9"
  },
  "keywords": [
    "akash",
    "akash-network",
    "cli",
    "command-line",
    "terminal",
    "blockchain",
    "cosmos",
    "web3",
    "decentralized-cloud",
    "deployment",
    "devops",
    "sdl",
    "docker",
    "kubernetes",
    "typescript"
  ],
  "homepage": "https://github.com/cryptoandcoffee/akash-jsdk/tree/main/packages/cli#readme",
  "bugs": {
    "url": "https://github.com/cryptoandcoffee/akash-jsdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cryptoandcoffee/akash-jsdk.git",
    "directory": "packages/cli"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "CryptoAndCoffee",
    "url": "https://github.com/cryptoandcoffee"
  },
  "publishConfig": {
    "access": "public"
  }
}
