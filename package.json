{
  "name": "akash-jsdk",
  "version": "1.0.0",
  "description": "Modern JavaScript SDK for Akash Network",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "build": "pnpm run --recursive build",
    "dev": "pnpm run --recursive dev",
    "test": "node scripts/check-dependencies.js && pnpm run --recursive test",
    "test:ci": "pnpm run --recursive test",
    "lint": "pnpm run --recursive lint",
    "format": "pnpm run --recursive format",
    "typecheck": "pnpm run --recursive typecheck",
    "clean": "pnpm run --recursive clean",
    "release": "pnpm publish -r --access public",
    "release:tag": "changeset tag",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "check-deps": "node scripts/check-dependencies.js"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "isomorphic-ws": "^5.0.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": [
    "akash",
    "akash-network",
    "blockchain",
    "cosmos",
    "cosmos-sdk",
    "web3",
    "decentralized-cloud",
    "cloud-computing",
    "deployment",
    "docker",
    "kubernetes",
    "IBC",
    "staking",
    "jwt-auth",
    "typescript",
    "javascript",
    "sdk"
  ],
  "homepage": "https://github.com/cryptoandcoffee/akash-jsdk#readme",
  "bugs": {
    "url": "https://github.com/cryptoandcoffee/akash-jsdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cryptoandcoffee/akash-jsdk.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "CryptoAndCoffee",
    "url": "https://github.com/cryptoandcoffee"
  },
  "dependencies": {
    "@types/elliptic": "^6.4.18",
    "@types/jsonwebtoken": "^9.0.10",
    "elliptic": "^6.6.1",
    "jsonwebtoken": "^9.0.2"
  }
}
